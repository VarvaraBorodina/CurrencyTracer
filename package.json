{
  "name": "currencytracer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "jest": "jest",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install",
    "cypress": "cypress open"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "automock": false,
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.svg": "<rootDir>/__mocks__/svg.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VarvaraBorodina/CurrencyTracer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VarvaraBorodina/CurrencyTracer/issues"
  },
  "homepage": "https://github.com/VarvaraBorodina/CurrencyTracer#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/axios": "^0.14.0",
    "@types/leaflet": "^1.9.3",
    "@types/recharts": "^1.8.24",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "cypress": "^12.17.1",
    "eslint-plugin-cypress": "^2.13.3",
    "file-loader": "^6.2.0",
    "jest": "^29.6.1",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-styled-components": "^7.1.1",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "recharts": "^2.7.2",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.8",
    "styled-components": "^6.0.2",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.3",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.6.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-styled-components-a11y": "^2.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.3",
    "typescript": "*",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
